OPENAI_API_KEY="your key"
OPENAI_MODEL="gpt-4o-mini"
AIR_AGENT_URL=http://finder.localhost:10002
RES_AGENT_URL=http://reserve.localhost:10002
WEA_AGENT_URL=http://localhost:10001

# ERC-8004 (optional)
ERC8004_ENABLED=true
ERC8004_RPC_URL="https://sepolia.infura.io/v3/<project>"
ERC8004_IDENTITY_REGISTRY="0x..."
ERC8004_REPUTATION_REGISTRY="0x..."

# Server agent keys (sign acceptFeedback)
ERC8004_PRIVATE_KEY_FINDER="0x..."
ERC8004_PRIVATE_KEY_RESERVE="0x..."

# Assistant key (used for signatures or deriving address only)
ERC8004_PRIVATE_KEY_ASSISTANT="0x..."

# Domains used to resolve IDs
FINDER_DOMAIN="finder.localhost:10002"
RESERVE_DOMAIN="reserve.localhost:10002"
ASSISTANT_DOMAIN="assistant.localhost:8083"

# Gas tuning (optional)
ERC8004_GAS_MULT=1.5
ERC8004_MIN_GAS=500000
ERC8004_GAS_PRICE_MULT=1.2
# or hard override in gwei
# ERC8004_GAS_PRICE_GWEI=5
